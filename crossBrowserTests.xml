    </test>
    <!--  Test  -->
    <test name="edgetest">
        <parameter name="os" value="Windows"/>
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="testCases.Authentication.TC1_registerUser"/>
        </classes>
    </test>
    <!--  Test  -->
    <test name="firefoxtest">
        <parameter name="os" value="Windows"/>
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="testCases.Authentication.TC1_registerUser"/>
        </classes>
    </test>
</suite>
